#!/bin/python

import unicodeit
import os
import subprocess
from time import sleep

string = subprocess.check_output(['xsel']).decode('utf-8')

command = 'xdotool type ' + unicodeit.replace(
    string.replace('\\phi', 'Ï†')
          .replace(' ', '\\ ')
          .replace('\\ff', 'Æ’')
          .replace('\\norm', '\|')
          .replace('\\exp', 'ğšğš¡ğš™')
          .replace('(', '\\(')
          .replace(')', '\\)')
    )

sleep(0.1)
os.system('xdotool keyup Super')
os.system(command)

